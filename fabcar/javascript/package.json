{
    "name": "fabcar",
    "version": "1.0.0",
    "description": "FabCar application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "start": "pm2 start app.js",
        "dev":"npx supervisor app.js"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.4",
        "crypto": "^1.0.1",
        "ejs": "^2.7.1",
        "express": "^4.17.1",
        "fabric-ca-client": "~1.4.0",
        "fabric-network": "^1.4.4",
        "firebase": "^7.7.0",
        "fs": "0.0.1-security",
        "http": "0.0.0",
        "js-sha256": "^0.9.0",
        "multer": "^1.4.2",
        "path": "^0.12.7",
        "shelljs": "^0.8.3",
        "socket.io": "^2.5.0",
        "ursa": "^0.9.4"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
